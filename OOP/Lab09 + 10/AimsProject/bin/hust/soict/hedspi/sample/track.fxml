<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hust.soict.hedspi.sample.Event">
   <children>
      <AnchorPane layoutX="1.0" prefHeight="400.0" prefWidth="400.0">
         <children>
            <Button fx:id="submit" layoutX="186.0" layoutY="339.0" mnemonicParsing="false" onAction="#submitTrack" text="Submit" />
            <Label layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="46.0" text="Length" />
            <TextField fx:id="lengthTrack" layoutX="83.0" layoutY="10.0" prefHeight="25.0" prefWidth="103.0" promptText="number" />
            <Label layoutX="14.0" layoutY="61.0" prefHeight="17.0" prefWidth="60.0" text="Title" />
            <TextField fx:id="titleTrack" layoutX="83.0" layoutY="57.0" prefHeight="25.0" prefWidth="103.0" promptText="Title" />
            <Button fx:id="addNewTrack" layoutX="35.0" layoutY="339.0" mnemonicParsing="false" onAction="#addTrack" text="Add Track" />
            <Button layoutX="156.0" layoutY="288.0" mnemonicParsing="false" onAction="#playCDTrack" prefHeight="25.0" prefWidth="60.0" text="Play" />
            <Label fx:id="printTrack" layoutX="17.0" layoutY="103.0" prefHeight="173.0" prefWidth="353.0" />
            <Button layoutX="294.0" layoutY="339.0" mnemonicParsing="false" onAction="#quit" text="Quit" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
